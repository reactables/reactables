---
import Layout from "../../layouts/Layout.astro";
import ContentPageLayout from "../../layouts/ContentPageLayout.astro";
import { getEntry } from "astro:content";

const reactBindings = await getEntry('react', 'react-bindings');
const { Content } = await reactBindings.render();

const sectionLinks =[
  {
    label: 'Installation',
    link: '#installation'
  },
  {
    label: 'Providers',
    link: '#providers'
  },
  {
    label: 'Hooks',
    link: '#hooks'
  },
];
---

<Layout title="Reactables - React Bindings">
  <ContentPageLayout
    sectionLinks={sectionLinks}
    editPageLink={{ link: 'https://github.com/reactables/reactables/edit/main/docs/src/content/react/react-bindings.md'}}
    prevLink={{label: 'Testing', link: '/guides/testing'}}
    nextLink={{label: 'React Form Components', link: '/react/react-form-components'}}
  >
    <Content />
  </ContentPageLayout>
</Layout>